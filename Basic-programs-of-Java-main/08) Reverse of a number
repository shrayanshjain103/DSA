package com.shrayansh;

import java.util.Scanner;

public class reverse {
    public static void main(String[] args) {
        Scanner in= new Scanner(System.in);
        System.out.print("enter a number to reverse: ");
        int n= in.nextInt();
        int a=0;
        int temp=n;
        while(temp>0) {
             a = temp % 10;
            temp = temp / 10;
            System.out.print(a);
        }
    }
}
