class HelloWorld {
    public static void main(String[] args) {
       pattern5(5);
    }
    static void pattern5(int n){
        for(int i=n;i>=1;i--){
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            System.out.println();
        }
    }
}
